{
  "name": "backlog-issue-viewer",
  "displayName": "backlog-issue-viewer",
  "description": "backlog-issue-viewer",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.59.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onView:backlogIssues",
    "onCommand:backlog-issue-viewer.setup"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "backlog-issue-viewer.setup",
        "title": "Setup Backlog Issue Viewer"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "backlogIssueViewer",
          "title": "Backlog Issue Viewer",
          "icon": "assets/Backlog_icon.png"
        }
      ]
    },
    "views": {
      "backlogIssueViewer": [
        {
          "id": "backlogIssues",
          "name": "Issues"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/node-fetch": "^2.5.12",
    "@types/vscode": "^1.59.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "prettier": "2.3.2",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "dependencies": {
    "node-fetch": "^2.6.1"
  }
}
